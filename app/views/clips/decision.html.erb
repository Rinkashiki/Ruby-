<%= stylesheet_link_tag 'users' %>
<div class="container form_padding">
  <br>
  <div class="form-group">
    <%= form_for @clip do |c| %>
      <h1>Change Decision</h1>
      <p class="custom-select">
        <%= c.label :decision %><br>
        <%= c.select :decision, @decisions.map{|d| [d.description]} %>
      </p>
      <p>
        <%= c.submit 'Cambiar', class: 'btn btn-primary' %>
      </p>
    <% end %>
  </div>
</div>